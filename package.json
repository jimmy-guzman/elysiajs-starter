{
  "name": "elysiajs-starter",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "deps:dedupe": "rm -rf node_modules && rm bun.lock && bun install",
    "deps:up": "bunx npm-check-updates -ui",
    "dev": "bun run --watch src/index.ts",
    "format": "biome format",
    "format:fix": "biome format --write",
    "postinstall": "lefthook install",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@elysiajs/cors": "1.4.0",
    "@elysiajs/openapi": "1.4.11",
    "@neondatabase/serverless": "1.0.2",
    "better-auth": "1.4.1",
    "drizzle-orm": "0.44.7",
    "elysia": "1.4.16",
    "env-schema": "7.0.0",
    "resend": "6.5.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@electric-sql/pglite": "0.3.14",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "bun-types": "1.3.3",
    "drizzle-kit": "0.31.7",
    "gitzy": "6.5.6",
    "lefthook": "2.0.4",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.13"
  },
  "packageManager": "bun@1.3.3"
}
